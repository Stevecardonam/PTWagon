{
  "builds": [
    {
      "src": "mobile/pubspec.yaml",
      "use": "@vercel/static-build",
      "config": {
        "root": "mobile",
        "installCommand": "npm install",
        "buildCommand": "flutter build --web-renderer=canvaskit --release",
        "outputDirectory": "build/web"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mobile/build/web/index.html"
    }
  ]
}